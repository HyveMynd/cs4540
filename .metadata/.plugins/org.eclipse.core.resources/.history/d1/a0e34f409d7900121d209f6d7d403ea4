<?php
require 'db.php';

if (isset($_REQUEST['resumeName'])){
	global $name;
	global $phone;
	
	
	$_SESSION['resumeName'] = $_REQUEST['resumeName'];
	$resumeName = $_REQUEST['resumeName'];
	storeResume($resumeName, $name, $phone, $addr, $position, $startdate, $enddate, $desc);
	echo "<script type=text/javascript >alert('Operation Successful')</script>";
	require 'archive.php';
}
?>